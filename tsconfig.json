{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@libs/helper": [
        "libs/helper/src"
      ],
      "@libs/helper/*": [
        "libs/helper/src/*"
      ],
      "@libs/core": [
        "libs/core/src"
      ],
      "@libs/core/*": [
        "libs/core/src/*"
      ],
      "@libs/utils": [
        "libs/utils/src"
      ],
      "@libs/utils/*": [
        "libs/utils/src/*"
      ],
      "@libs/cache": [
        "libs/cache/src"
      ],
      "@libs/cache/*": [
        "libs/cache/src/*"
      ],
      "@libs/database-typeorm": [
        "libs/database-typeorm/src"
      ],
      "@libs/database-typeorm/*": [
        "libs/database-typeorm/src/*"
      ],
      "libs/bullmq": [
        "libs/bullmq/src"
      ],
      "libs/bullmq/*": [
        "libs/bullmq/src/*"
      ]
    }
  }
}